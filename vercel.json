{
  "rewrites": [
    {
      "source": "/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "missing",
          "value": "file"
        },
        {
          "type": "missing",
          "value": "directory"
        }
      ],
      "destination": "/index.html"
    }
  ]
}
